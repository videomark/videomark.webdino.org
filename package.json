{
  "name": "videomark.webdino.org",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "check:eslint": "eslint --ignore-path .gitignore .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "build": "MODE=production vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/d3-array": "^3.2.2",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-selection": "^3.0.11",
    "@types/md5": "^2.3.6",
    "@types/node": "^25.0.3",
    "@types/topojson-client": "^3.1.5",
    "cspell": "^9.4.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-svelte": "^2.46.1",
    "intl-messageformat": "^11.0.8",
    "marked": "^17.0.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "sass": "^1.97.1",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-scss": "^6.14.0",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "vite": "^7.3.0",
    "yaml": "^2.8.2"
  },
  "type": "module",
  "dependencies": {
    "@mongodb-js/charts-embed-dom": "^3.3.1",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "md5": "^2.3.0",
    "topojson-client": "^3.1.0"
  }
