{
  "name": "videomark.webdino.org",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "cms": "netlify-cms-proxy-server",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "check": "pnpm run '/^check:.*/'",
    "check:audit": "pnpm audit",
    "check:cspell": "cspell --no-progress",
    "check:svelte": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "check:eslint": "eslint --ignore-path .gitignore .",
    "check:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "build": "MODE=production vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/d3-array": "^3.2.1",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-geo": "^3.1.0",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.11",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.10.2",
    "@types/topojson-client": "^3.1.5",
    "cspell": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-svelte": "^2.46.1",
    "intl-messageformat": "^10.7.10",
    "marked": "^15.0.4",
    "netlify-cms-proxy-server": "^1.3.24",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "sass": "^1.83.0",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.10.0",
    "svelte": "^5.16.0",
    "svelte-check": "^4.1.1",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "vite": "^6.0.6",
    "yaml": "^2.6.1"
  },
  "type": "module",
  "dependencies": {
    "@mongodb-js/charts-embed-dom": "^3.3.1",
    "d3-array": "^3.2.4",
    "d3-axis": "^3.0.0",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "md5": "^2.3.0",
    "topojson-client": "^3.1.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}
